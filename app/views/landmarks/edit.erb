<h1>Edit a Landmark</h1>

<form action="/landmarks/<%= @landmark.id %>" method="POST">
  <input type="hidden" name="_method" value="PATCH">

  <p>Landmark Name:<input type="text" name="name" id="name" value="<%= @landmark.name %>"></p>
  <p>Year Completed:<input type="text" name="year_completed" id="year_completed" value="<%= @landmark.year_completed %>"</p>

  <input type="submit" value="Edit Landmark">
</form>
